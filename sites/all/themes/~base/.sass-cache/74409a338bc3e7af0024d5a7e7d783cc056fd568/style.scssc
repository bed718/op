:@template",@import "compass";
@import "_reset";
// @import "_normalize";
@import "_base";



/* COMMON 
-------------------------------------------------------------- */
body{
	@include font;
	background-color: $body_bg;  
	color: $text_main;
	font-size: 18px;
	font-weight: 400;
}

a{
	@include link_trans;
	color: $text_accent;
	text-decoration: none;
	
	&:hover{color: $text_main;}
} 

li a.active,
li.opened span.nolink{color: $text_main;}

h1, h2, h3, h4{
	font-weight: 200;
}

h2{
	font-weight: 400;
}

h2.pane-title{
	font-family: $font_fancy;
	font-size: 36px;
	margin-bottom: 20px;
	text-align: center;
	letter-spacing: 2px;
}


p{
	font-size: 16px;
	line-height: 22px;
	font-weight: 300;
}

ul,
ul.menu,
ul.menu li{
	list-style: none;
	text-align: inherit;
	margin: 0;
	padding: 0;
}

*{
	@include bp(m){
		img{
			max-width: $mobile_width;
			height: auto;
		}
	}
}


/* HEADER 
-------------------------------------------------------------- */
#header{
	
	width: 100%;
	z-index: 99;

	.divider{
		margin-top: 100px;
	}

	@include bp(m){
		position: relative;

		.divider{
			display: none;
		}
	}
}

#header-inner{
	position: relative;
	
	max-width: 1500px;
	margin: auto;
}

#logo{
	position: relative;
	text-align: center;
	margin-top: 150px;
	z-index: 10;

	a{
		font-family: $font_header;
		font-size: 50px;
		letter-spacing: 20px;
		text-transform: uppercase;
		color: $text_main;	

		span{
			font-family: $font_fancy;
			text-transform: lowercase;
		}

		&:hover{
			color: $text_accent;
		};
	}

	@include bp(m){
		margin-top: 0;
		text-align: center;

		a{
			width: 100%;
			font-size: 20px;
			padding: 10px;
			letter-spacing: 2px;
			display: block;

			span{
				display: inline;
				margin: 0 10px;
			}
		}
	}
}

#nav{
	position: fixed;
	width: 100%;
	z-index: 1;
	top: 200px;
	left: 0;
	z-index: 99;
	background-color: #fff;

	@include bp(m){
		height: auto;	
		text-align: center;	
		position: relative;
		top: auto !important;
		left: auto;	
	}
}

#block-system-main-menu{
	text-align: center;

	@include bp(m){
		position: relative;
		right: auto;
	}

	ul{
		font-size: 0;
		position: relative;
	}

	li{
		@extend .inline_block;
		list-style: none;
		position: relative;
		margin: 0;
		padding: 0;

		@include bp(m){
			float: none;
			display: inline-block;
		}

		ul{
			display: none;
			position: absolute;
			top: 25px;
			
			left: 0;
			width: 335px;
			
			li{
				display: block;
				float: none;
			}

			a{
				display: block;
			}

		}

		&:hover ul{
			display: block;
		};


		span, a{
			@include link_trans;
			@extend .inline_block;
			font-size: 14px;
			text-transform: uppercase;
			letter-spacing: 2px; 			
			font-weight: 400;
			margin: 0 20px;
			cursor: pointer;
			padding: 20px 0;
			
			&:hover{
				color: $text_main;
			}
			
			&.active,
			&.active-trail{
				color: $text_main;
			}

			@include bp(m){
				padding-bottom: 20px;
				padding-top: 20px;
				font-size: 12px;
				letter-spacing: 0;
				margin: 0 10px;
			}

		}

	}
}

.menu-toggle{
	display: none;
	cursor: pointer;
}

.divider{
	color: $text_accent;
	padding: 40px 0;
	text-align: center;
	width: 100px;
	height: 1px;
	margin: 40px auto;
	
	span{
		width: 100%;
		height: 1px;
		display: block;
		background-color: $text_accent;
		-webkit-transform: rotate(-30deg);
		-moz-transform: rotate(-30deg);
		-ms-transform: rotate(-30deg);
		-o-transform: rotate(-30deg);
		filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=0);
	}

	@include bp(m){
		padding: 20px 0;
	}
}

.btn-main{
	text-align: center;
	display: block;
	position: relative;
	margin-top: 20px;

	a{
		@extend .inline_block;
		padding: 20px 40px;
		text-transform: uppercase;
		color: $text_main;
		font-size: 12px;
		letter-spacing: 2px;
		border: $text_accent solid 1px;
		font-weight: 700;
		
		&:hover{
			background-color: $text_main;
			border: $text_main solid 1px;
			color: #fff;
		};
	}
}



/* MAIN 
-------------------------------------------------------------- */
#main-content{
	z-index: 1;
	position: relative;
	margin-top: 60px;

	@include bp(m){
		margin-top: 0;	
	}
}


#main{
	max-width: 1000px;
	margin: auto;
	overflow: hidden;
}



/* IMAGES
-------------------------------------------------------------- */
.gallery-featured{

	.images{
		text-align: center;
		width: 1000px;
		height: 650px;
		position: relative;

		img{
			width: 100%;
			height: auto;
			position: absolute;
			top: 0;
			left: 0;
			cursor: pointer;
		}
	}

	@include bp(m){
		.images{
			width: 100%;
			height: auto;
			position: relative;

			img{
				width: $mobile_width;
				height: auto;
				position: relative;
				top: auto;
				margin: 0 auto 10px;
				left: auto;
				cursor: default;
				display: block;
			}
		}
	}
}


/* PAGE
-------------------------------------------------------------- */
.field-name-field-header-image{
	text-align: center;
	margin-bottom: 20px;
}

.bio-page{

	p{
		max-width: 700px;
		margin: 0 auto 20px;

		@include bp(m){
			max-width: $mobile_width;
		}
	}
}


/* RESUME
-------------------------------------------------------------- */
.field-name-field-resume-section{
	max-width: 700px;
	margin: 0 auto 40px;
	padding-bottom: 40px;
	border-bottom: $text_accent dashed 1px;
}


.field-name-field-project-title,
.field-name-field-project-position,
.field-name-field-project-studio{
	float: left;
	width: 33%;
	text-transform: uppercase;
	font-weight: 300;
	font-size: 14px;

	@include bp(m){
		float: none;
		width: 100%;
		text-align: center !important;
		margin-bottom: 5px;
	}
}

.field-name-field-project-title{
	font-weight: 400;
}

.field-name-field-project-studio{
	text-align: right;
}

.field-name-field-project-position{
	text-align: center;
}

.field-name-field-resume-item{
	@extend .clearfix;
	position: relative;
	margin-bottom: 10px;

}

.field-name-field-resume-item + .field-name-field-resume-item{
	@include bp(m){
		padding-top: 20px;
		margin-top: 20px;
		border-top: lighten($text_accent, 15%) solid 1px;
	}
}

.field-name-field-clip-reference{
	
	position: absolute;
	top: 0;
	left: 720px;
	width: 100px;
	text-transform: uppercase;
	font-size: 10px;
	padding-top: 2px;

	a{
		@extend .link_switch;
		padding: 2px 5px;
		border: $text_accent solid 1px;

		&:hover{
			background-color: $text_main;
			border: $text_main solid 1px;
			color: #fff;
		};
	}

	@include bp(m){
		@extend .inline_block;
		position: relative;
		top: auto;
		left: auto;
		text-align: center;
		width: 100%;
		margin-top: 20px;
	}
}

h2.field-name-field-section-title{
	@extend .header_main;
}


/* POST
-------------------------------------------------------------- */
.post{
	max-width: $post_width;
	margin: auto;

	.post-date{
		text-align: center;
		font-family: $font_header;
		font-size: 36px;
		margin-bottom: 20px;

		span{
			font-family: $font_fancy;
			margin-left: 10px;
		}
	}

	.image-holder{
		text-align: center;
		margin-bottom: 20px;
	}

	.body{
		text-align: justify;

		p{
			font-weight: 300;
			margin-bottom: 20px;
		}
	}

	@include bp(m){
		
		.body{
			max-width: $mobile_width;
			margin: auto;
		}
	}
}

.post-teaser{
	border-bottom: $text_accent dashed 1px;
	padding-bottom: 40px;
	margin-bottom: 40px;
}

/* VIDEO 
-------------------------------------------------------------- */
.video-teaser{
	width: 48%;
	float: left;
	margin: 0 1% 40px;
	text-align: center;

	h2{
		margin-top: 5px;

		a{
			text-transform: uppercase;
			color: $text_main;
			font-size: 14px;

			&:hover{
				color: $text_accent;
			};
		}
	}

	.image{

		img{
			width: 100%;
			height: auto;
		}
	}

	@include bp(s){
		width: 100%;
		float: none;
		margin: 0 0 40px;
	}
}

.video-full{
	width: 640px;
	margin: auto;

	h2{
		font-family: $font_fancy;
		font-size: 36px;
		margin-bottom: 20px;
		text-align: center;
		letter-spacing: 2px;
	}

	.video{
		margin-bottom: 20px;
	}

	.body{
		text-align: justify;
		font-weight: 300;
	}
}

.field-name-field-video-url{
	@include bp(m){
		text-align: center;
		img{
			width: $mobile_width;
			height: auto;
			margin: auto;
		}
	}
}

/* IMAGES 
-------------------------------------------------------------- */
.gallery{

	.images{

		.field{
			width: 32%;
			padding: .5%;
			img{
				width: 100%;
				height: auto;
			}
		}
	}

	h2{
		font-family: $font_fancy;
		font-size: 36px;
		margin-bottom: 20px;
		text-align: center;
		letter-spacing: 2px;

		a{
			@extend .link_switch;
		}
	}


	@include bp(m){
		.images{

			.field{
				width: 50%;
				padding: 1%;
				img{
					width: 100%;
					max-width: 100%;
					height: auto;
				}
			}
		}
	}
}

/* FOOTER
-------------------------------------------------------------- */

#footer { 
	text-align: center;
	padding-top: 80px;
	
	h2{
		font-family: $font_header;
		font-size: 36px;
		margin-bottom: 20px;
	}
}

#footer-main{
	background-color: $text_main;
	padding: 20px;
	letter-spacing: 1px;
	text-transform: uppercase;
	color: #fff;
	font-weight: 300;

	a{
		font-size: 16px;

		&:hover{
			color: #fff;
		};
	}

	strong{
		font-weight: 400;
	}

	#follow{
		margin-bottom: 10px;
	}
	
	.phone{
		margin-top: 20px;
	}

}

#footer-bottom{
	background-color: lighten($text_main, 10%);
	padding: 20px;
	font-size: 10px;
	text-transform: uppercase;
	letter-spacing: 2px;
	color: $text_accent;
}

/* SOCIAL
-------------------------------------------------------------- */
.social-icons{

	.icon{
		@include border-radius($socail_width / 2);
		background: transparent url(../images/bb_social.png) no-repeat;
		height: $socail_width;
		width: $socail_width;
		display: inline-block;
		
		margin: 5px;

		@include transition;

		&:hover{background-color: $text_accent;}
	}

	.twitter{background-position: -$socail_width 0;}
	.instagram{background-position: -($socail_width * 2) 0;}
	.youtube{background-position: -($socail_width * 3) 0;}
	.pinterest{background-position: -($socail_width * 4) 0;}
}



/* FORMS
-------------------------------------------------------------- */
form{
	width: 600px;
	margin: 40px auto 0;

	.form-item .description{
		color: $text_accent;
		font-size: 12px;
		margin-top: 3px;
	}
}

.form-item{margin-top: 0;}

input[type="text"],
input[type="password"],
input[type="email"],
textarea{
	width: 100%;
	padding: 5px;
	border: none;
	font-size: 18px;
	font-weight: 200;
	background-color: darken($body_bg, 10%);
	box-shadow: 1px 1px 1px #ccc inset;

	&:focus{
		background-color: darken($page_bg, 5%);
		color: $text_main;
	};
}

label{
	font-size: 16px;
	text-transform: uppercase;
	margin-bottom: 3px;
}

input[type="submit"]{
	background-color: $text_main;
	color: #fff;
	padding: 10px 20px ;
	cursor: pointer;
	text-transform: uppercase;
	font-size: 18px;
	letter-spacing: 2px;
	font-weight: 200;
	border: none;
	margin-left: 5px;

	&:hover{
		background-color: darken($page_bg, 10%);
	};
}

.form-actions{
	text-align: right;
	margin-bottom: 0;
}

ul.tabs{
	text-align: center;
	margin-top: 20px;
}




/* EXTRAS
-------------------------------------------------------------- */
body.cke_show_borders{width: $post_width !important; min-width: $post_width; margin: 0 auto;}

.image-post_full {
	position: relative;
	z-index: 1;
}

#cboxContent, .cboxPhoto{
	border-radius: 0 !important;
}

#cboxClose.cbox-close-plain{
	border-radius: 0 !important;
	top: 0;
	right: 0;
	width: 30px;
	height: 30px;
	font-size: 14px;
}








